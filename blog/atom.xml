---
layout: null
---

<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="https://www.w3.org/2005/Atom">

	<title>Marc's Blog</title>
	<link href="https://gironslopez.github.io/blog/atom.xml" rel="self"/>
	<link href="https://gironslopez.github.io/blog"/>
	<updated>{{ site.time | date_to_xmlschema }}</updated>
	<id>https://gironslopez.github.io/blog</id>
	<author>
		<name>Marc Girons Lopez</name>
		<email>m.girons@gmail.com</email>
	</author>

	{% for post in site.posts %}
		<entry>
			<title>{{ post.title }}</title>
			<link href="https://gironslopez.github.io{{ post.url }}"/>
			<updated>{{ post.date | date_to_xmlschema }}</updated>
			<id>https://gironslopez.github.io/{{ post.id }}</id>
			<content type="html">{{ post.content | xml_escape }}</content>
		</entry>
	{% endfor %}

</feed>
